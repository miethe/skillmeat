{
  "name": "skillmeat-web",
  "version": "0.3.0-alpha",
  "description": "SkillMeat Web Interface - Collection browser and management dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "generate-sdk": "npx openapi-typescript-codegen --input ../api/openapi.json --output ./sdk --client fetch --name SkillMeatClient --useOptions --useUnionTypes"
  },
  "dependencies": {
    "next": "^15.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "lucide-react": "^0.451.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "typescript": "^5.6.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "postcss": "^8.4.48",
    "autoprefixer": "^10.4.20",
    "openapi-typescript-codegen": "^0.29.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
