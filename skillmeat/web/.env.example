# SkillMeat Web UI Environment Variables
# Copy this file to .env.local and update with your configuration

# FastAPI Backend URL
NEXT_PUBLIC_API_URL=http://localhost:8080

# Application Settings
NEXT_PUBLIC_APP_NAME=SkillMeat
NEXT_PUBLIC_APP_VERSION=0.3.0-alpha

# Optional authentication
# NEXT_PUBLIC_API_TOKEN=
# NEXT_PUBLIC_API_KEY=

NEXT_PUBLIC_ENABLE_API_MOCKS=false
NEXT_PUBLIC_API_TRACE=true

# Optional: Authentication (future)
# NEXT_PUBLIC_AUTH_ENABLED=false
